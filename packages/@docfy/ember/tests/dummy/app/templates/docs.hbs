<h1>Docs</h1>

<ul>
  {{#each this.model as |page|}}
    <li>
      <DocfyLink @to={{page.url}}>
        {{page.title}}
      </DocfyLink>
      {{#if true}}
        {{#if page.headings.length}}
          <ul>
            {{#each page.headings as |heading|}}
              <li>
                <DocfyLink @to={{page.url}} @anchor={{heading.id}}>
                  {{heading.title}}
                </DocfyLink>
              </li>
            {{/each}}
          </ul>
        {{/if}}
      {{/if}}
    </li>
  {{/each}}
</ul>

{{outlet}}
