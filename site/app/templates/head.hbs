<meta property="og:description" content="Docfy is a modular JavaScript tool to help build documentation sites.">

<meta name="theme-color" content="#12161f">
<meta property="og:type" content="website">
<meta property="og:url" content="/assets/images/docfy-social-media.jpg">
<meta property="og:image:type" content="image/jpeg">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:creator" content="@josemarluedke">
